    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@tailwindcss/vite": "^4.0.17",
    "arctic": "^3.6.0",
    "astro": "^5.5.5",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.41.0",
    "react": "^19.0.0",
    "react-aria-components": "^1.7.1",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.17"
  },
