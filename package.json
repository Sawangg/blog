{
  "name": "blog",
  "author": "Sawangg",
  "type": "module",
  "version": "0.0.2",
  "scripts": {
    "dev": "astro dev",
    "build": "bun run check && astro check && astro build",
    "deploy:staging": "bun run check && astro check && astro build && wrangler pages deploy --branch staging",
    "deploy:prod": "bun run check && astro check && astro build --mode staging && wrangler pages deploy",
    "check": "biome check",
    "check:fix": "biome check --fix --unsafe",
    "db:push": "NODE_ENV=development drizzle-kit push",
    "db:studio": "NODE_ENV=development drizzle-kit studio --host localhost --port 3001"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.1.0",
    "@astrojs/mdx": "^4.0.3",
    "@astrojs/react": "^4.1.2",
    "@astrojs/rss": "^4.0.10",
    "@astrojs/tailwind": "^5.1.4",
    "@neondatabase/serverless": "^0.10.4",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "arctic": "^2.3.3",
    "astro": "^5.1.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.2",
    "react": "^19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.2.1",
    "@biomejs/biome": "^1.9.4",
    "@playform/compress": "^0.1.6",
    "@types/bun": "^1.1.14",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "astro-robots-txt": "^1.0.0",
    "drizzle-kit": "^0.30.1",
    "typescript": "^5.7.2",
    "wrangler": "^3.99.0"
  }
}
